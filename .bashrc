
###### Aliases ######

# bash abbreviations
alias ls="ls -F"
alias la="ls -aF" 
alias l="ls -lF"
alias ll="ls -laF"
alias copy="pbcopy"
alias c="clear"

# Git grep
alias gg="git grep --break --heading --line-number"

alias top="top -o cpu"

# meta
alias vbrc="vim ~/.bashrc"
alias vvrc="vim ~/.vimrc"
alias sbrc="source ~/.bashrc"
